JSONSetElement ( "{}" 

	;[  "SCRIPT_LOG_UUID" 	; $script_log_uuid  				; JSONString]
	;[  "STOPPED_TIMESTAMP" ; Get(CurrentHostTimestamp) 		; JSONNumber ]
	;[  "STOPPED_UTC_MS" 	; Get(CurrentTimeUTCMilliseconds) 	; JSONNumber ]
	;[  "EXIT_SCRIPT_OBJECT" ;  ExitScriptObject			; JSONObject ]

)
